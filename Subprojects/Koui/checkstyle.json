{
	"defaultSeverity": "INFO",
	"checks": [
		{
			"type": "CodeSimilarity"
		},
		{
			"type": "DefaultComesLast"
		},
		{
			"type": "DocCommentStyle",
			"props": {
			  "lineStyle": "onestar",
			  "startStyle": "ignore"
			}
		},
		{
			"type": "ERegLiteral"
		},
		{
			"props": {
				"tokens": [
					"CLASS_DEF",
					"ENUM_DEF",
					"ABSTRACT_DEF",
					"TYPEDEF_DEF",
					"INTERFACE_DEF",
					"OBJECT_DECL",
					"FUNCTION",
					"FOR",
					"IF",
					"WHILE",
					"SWITCH",
					"TRY",
					"CATCH"
				],
				"option": "empty"
			},
			"type": "EmptyBlock"
		},
		{
			"props": {
				"requireEmptyLineAfterClass": false,
				"max": 1
			},
			"type": "EmptyLines"
		},
		{
			"props": {
				"option": "lowerCase"
			},
			"type": "HexadecimalLiteral"
		},
		{
			"type": "IndentationCharacter",
			"props": {
				"severity": "INFO",
				"character": "tab"
			}
		},
		{
			"type": "InnerAssignment"
		},
		{
			"type": "LineLength",
			 "props": {
				 "severity": "ERROR",
				 "max": 160
			 }
		},
		{
			"props": {
				"modifiers": [
					"MACRO",
					"PUBLIC_PRIVATE",
					"OVERRIDE",
					"STATIC",
					"INLINE",
					"DYNAMIC"
				]
			},
			"type": "ModifierOrder"
		},
		{
			"type": "MultipleVariableDeclarations"
		},
		{
			"props": {
				"allowSingleLineStatement": true,
				"tokens": [
					"FOR",
					"IF",
					"ELSE_IF",
					"WHILE",
					"DO_WHILE"
				]
			},
			"type": "NeedBraces"
		},
		{
			"props": {
				"assignOpPolicy": "around",
				"unaryOpPolicy": "none",
				"ternaryOpPolicy": "around",
				"arithmeticOpPolicy": "around",
				"compareOpPolicy": "around",
				"bitwiseOpPolicy": "around",
				"boolOpPolicy": "around",
				"intervalOpPolicy": "none",
				"arrowPolicy": "around",
				"oldFunctionTypePolicy": "none",
				"newFunctionTypePolicy": "around",
				"arrowFunctionPolicy": "around"
			},
			"type": "OperatorWhitespace"
		},
		{
			"props": {
				"tokens": [
					"=",
					"*",
					"/",
					"%",
					">",
					"<",
					">=",
					"<=",
					"==",
					"!=",
					"&",
					"|",
					"^",
					"<<",
					">>",
					">>>",
					"+=",
					"-=",
					"*=",
					"/=",
					"%=",
					"<<=",
					">>=",
					">>>=",
					"|=",
					"&=",
					"^=",
					"...",
					"=>",
					"++",
					"--",
					"+",
					"-",
					"&&",
					"||"
				],
				"option": "nl"
			},
			"type": "OperatorWrap"
		},
		{
			"type": "RedundantModifier"
		},
		{
			"type": "RedundantAllowMeta"
		},
		{
			"type": "RedundantAccessMeta"
		},
		{
			"props": {
				"dotPolicy": "none",
				"commaPolicy": "after",
				"semicolonPolicy": "after"
			},
			"type": "SeparatorWhitespace"
		},
		{
			"props": {
				"tokens": [
					","
				],
				"option": "eol"
			},
			"type": "SeparatorWrap"
		},
		{
			"props": {
				"spaceIfCondition": "should",
				"spaceAroundBinop": true,
				"spaceForLoop": "should",
				"ignoreRangeOperator": true,
				"spaceWhileLoop": "should",
				"spaceCatch": "should",
				"spaceSwitchCase": "should",
				"noSpaceAroundUnop": true
			},
			"type": "Spacing"
		},
		{
			"props": {
				"allowException": true,
				"policy": "doubleAndInterpolation"
			},
			"type": "StringLiteral"
		},
		{
			"props": {
				"format": "TODO|FIXME|XXX",
				"severity": "INFO"
			},
			"type": "TODOComment"
		},
		{
			"type": "TrailingWhitespace"
		},
		{
			"type": "UnusedImport"
		},
		{
			"type": "UnusedLocalVar"
		},
		{
			"props": {
				"tokens": [
					",",
					";",
					":"
				]
			},
			"type": "WhitespaceAfter"
		},
		{
			"props": {
				"tokens": [
					"=",
					"+",
					"-",
					"*",
					"/",
					"%",
					">",
					"<",
					">=",
					"<=",
					"==",
					"!=",
					"&",
					"|",
					"^",
					"&&",
					"||",
					"<<",
					">>",
					">>>",
					"+=",
					"-=",
					"*=",
					"/=",
					"%=",
					"<<=",
					">>=",
					">>>=",
					"|=",
					"&=",
					"^=",
					"=>"
				]
			},
			"type": "WhitespaceAround"
		}
	]
}
